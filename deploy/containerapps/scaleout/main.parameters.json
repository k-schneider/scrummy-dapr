{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "canadacentral"
    },
    "resourceBaseName": {
      "value": "scrummyscale"
    },
    "cosmosEnableFreeTier": {
      "value": true
    },
    "containerRegistry": {
      "value": "ghcr.io"
    },
    "gameServiceContainerImage": {
      "value": "k-schneider/scrummy-dapr/game.service:main"
    },
    "gameServiceMaxReplicas": {
      "value": 3
    },
    "gameServiceScaleConcurrentRequests": {
      "value": "100"
    },
    "webBlazorContainerImage": {
      "value": "k-schneider/scrummy-dapr/web.blazor:main"
    },
    "webBlazorMaxReplicas": {
      "value": 3
    },
    "webBlazorScaleConcurrentRequests": {
      "value": "100"
    }
  }
}
